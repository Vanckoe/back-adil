{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { mapActions, mapState } from \"vuex\";\nexport default {\n  name: \"ProductData\",\n  data() {\n    return {\n      productId: this.$route.params.id,\n      editing: false,\n      editedProduct: {}\n    };\n  },\n  computed: {\n    ...mapState('productsdata', ['products']),\n    productById() {\n      return this.products.find(p => p.id == this.productId);\n    }\n  },\n  methods: {\n    ...mapActions('productsdata', ['UPDATE_PRODUCT_BY_ID', 'DELETE_PRODUCT']),\n    beginEdit() {\n      this.editing = true;\n      this.editedProduct = {\n        ...this.productById\n      };\n    },\n    async saveEdit() {\n      await this.UPDATE_PRODUCT_BY_ID({\n        id: this.productId,\n        updatedProductData: {\n          title: this.editedProduct.title\n        }\n      });\n      this.editing = false;\n    },\n    cancelEdit() {\n      this.editing = false;\n      this.editedProduct = {};\n    },\n    async deleteProduct() {\n      await this.DELETE_PRODUCT(this.productId);\n      this.$router.push({\n        name: 'ProductsData'\n      });\n    }\n  }\n};","map":{"version":3,"names":["mapActions","mapState","name","data","productId","$route","params","id","editing","editedProduct","computed","productById","products","find","p","methods","beginEdit","saveEdit","UPDATE_PRODUCT_BY_ID","updatedProductData","title","cancelEdit","deleteProduct","DELETE_PRODUCT","$router","push"],"sources":["C:\\Users\\Vanckoe\\OneDrive\\Documents\\work\\Noncomercial\\lara\\crud\\src\\components\\products\\ProductData.vue"],"sourcesContent":["<template>\r\n  <div class=\"container mt-5\" v-if=\"productById\">\r\n    <div class=\"row\">\r\n      <div class=\"col-lg-4\">\r\n        <img :src=\"productById.thumbnail\" class=\"img-fluid\" alt=\"Product Thumbnail\">\r\n      </div>\r\n      <div class=\"col-lg-8\">\r\n        <h2 class=\"mb-3\">{{productById.title}}</h2>\r\n        <p><strong>Description:</strong> {{productB